{
	"compilerOptions": {
		"module": "commonjs",
		"declaration": false,
		"noImplicitAny": false,
		"removeComments": true,
		"noLib": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"target": "es6",
		"jsx": "react-native",
		"sourceMap": true,
		"allowJs": true,
		"outDir": "./dist"
	},
	"include": ["server/src/**/*"],
	"exclude": ["server/node_modules", "server/**/*.spec.ts"]
}
